{
    "input_transforms": [
        {
            "name": "Spectrogram",
            "parameters": {
                "n_fft": 512,
                "hop_length": 128,
                "window_fn": "sqrt_hann_window",
                "power": 1.0,
                "window_scaling": true
            }
        }
    ],

    "separator": {
        "model": "UNetSpectrogramSeparator",
        "input_transforms": [
            {
                "name": "LogMagnitude",
                "parameters": {}
            }
        ],
        "parameters": {
            "n_blocks": 4,
            "init_channels": 16,
            "janssen_variant": true
        }
    },

    "losses": {
        "separation": {
            "energy_masking": true
        }
    },

    "training": {
        "batch_size": 8,
        "num_epochs": 50,
        "class_prior_weighting": true,
        "early_stopping_patience": 5,
        "early_stopping_terminate": true,
        "optimizer": {
            "name": "Adam",
            "parameters": {
                "lr": 0.00001
            }
        }
    }

}
